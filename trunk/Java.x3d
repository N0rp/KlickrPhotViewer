<Engine>
 <RenderJob>
	<WindowGroup>
	  <Window size='1000 500'/>
	</WindowGroup>
  </RenderJob>
</Engine>	
  
<Scene>
	// hoert auf Tastatursignale, damit der Benutzer mit der Tastatur nach Bildtags suchen kann
	<KeySensor DEF='keyboard'/>
	
	////////////////////////////////////////////////////////////////
	// Etwas Hintergrundmusik macht das ganze etwas proffesionell //
	////////////////////////////////////////////////////////////////
	<!--
	<Transform translation='0 0 0'>	
		<Sound location='0 0 0' priority='1.0' direction='0 0 0' minBack='1000' minFront='1000' maxBack='1000' maxFront='1000'>
			<AudioClip enabled='true' loop='true' url='"../music/69-1-1.wav" "../music/69-2-1.wav"'/>
		</Sound>

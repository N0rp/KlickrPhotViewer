	//////////////////////////////////////////
	// Winkel-Werte 1. 0 1 0 0				//
	//				2. 0 1 0 1.57			//
	//				3. 0 1 0 3.14159		//
	//				4. 0 1 0 4.71159		//
	//				5. 0 1 0 6.28318		//
	//////////////////////////////////////////
	// imagebox-Nummer faengt bei 0 an da es so besser mit dem array-Index korespondiert
	//////////////////
	// ImageBox - 0 //
	//////////////////
	<Transform DEF='box_0' translation='-4.5 3 0'>
		<Shape>
	    	<Box
				size='2 2 0.1'
				useGeoCache='TRUE'
				solid='TRUE'
				lit='TRUE'
				resolution='1'
				multiResolutionLevel='1'
				invalidateVolume='FALSE'
				normalUpdateMode='nice'
				cacheMode='auto'
				optimizationMode='auto'
				logFeature=''
			/>
			<Appearance>
				<Material diffuseColor='0.2 0.3 0.2'/>
				<ImageTexture DEF='image_change_0' url="../img/standard.jpg"/>
			</Appearance>
		</Shape>
		
		<TouchSensor DEF='touch_me_0'/>
		
		<TimeSensor DEF='zeit_box_0_links' cycleInterval='3' loop='false'/>
		<TimeSensor DEF='zeit_box_0_rechts' cycleInterval='3' loop='false'/>
		
		<TimeSensor DEF='box_0_zeit_minmax' cycleInterval='0.3' loop='false'/>
		
		<OrientationInterpolator DEF='drehung_box_0_rechts' 
			key='0 0.25 0.5 0.75 1'
			keyValue='0 1 0 0, 0 1 0 1.57, 0 1 0 3.14159, 0 1 0 3.14159, 0 1 0 3.14159'
		/>
		<OrientationInterpolator DEF='drehung_box_0_links' 
			key='0 0.25 0.5 0.75 1'
			keyValue='0 1 0 3.14159, 0 1 0 1.57, 0 1 0 0, 0 1 0 0, 0 1 0 0'
		/>
		//KeyValues sind egal da sie durch Java gesetzt werden
		<PositionInterpolator DEF='box_0_groesse' 
			key='0 1'
			keyValue='1 1 0.1, 3 2.7 0.1'
		/>
		<PositionInterpolator DEF='box_0_position' 
			key='0 1'
			keyValue='-4.5 3 0, 0 0.35 3'
		/>
		
	</Transform>
	// Drehen rechts, wenn ein Zeitevent per Mouse-Click auf den linken Button oder mit der Kinect generiert wird
	<!--<ROUTE fromNode='touch_me_0' 			fromField='touchTime' 			toNode='zeit_box_0_max'		toField='startTime'/>-->
 	//Routen zum maximieren/minimieren
	<ROUTE fromNode='box_0_zeit_minmax' 	fromField='fraction_changed' 	toNode='box_0_groesse' 	toField='set_fraction'/>
	<ROUTE fromNode='box_0_zeit_minmax' 	fromField='fraction_changed' 	toNode='box_0_position' toField='set_fraction'/>
 	<ROUTE fromNode='box_0_groesse' 		fromField='value_changed' 		toNode='box_0' 					toField='scale'/>
	<ROUTE fromNode='box_0_position' 		fromField='value_changed' 		toNode='box_0' 						toField='translation'/>
	// Drehen links, wenn ein Zeitevent per Mouse-Click auf den linken Button oder mit der Kinect generiert wird
 	<ROUTE fromNode='zeit_box_0_links' 		fromField='fraction_changed' 	toNode='drehung_box_0_links' 	toField='set_fraction'/>
 	<ROUTE fromNode='drehung_box_0_links' 	fromField='value_changed' 		toNode='box_0' 					toField='rotation'/>
	
	

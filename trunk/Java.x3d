	///////////////////////////////////////////////////////////////
	// Dieser Blickwinckel ist gut beim Vollbildmodus, dann wird //
	// alles ordentlich angezeigt 								 //
	///////////////////////////////////////////////////////////////
	<Viewpoint DEF='vp1' position='0 0 11'/>
	
	
	///////////////////
	// Kinect Sensor //
	///////////////////
    <IOSensor DEF='kinect' type='NI'>
        <field name='depth' 		 	accessType='outputOnly' type='SFImage'/>
        <field name='JointPositions' 	accessType='outputOnly' type='MFVec3f'/>
		<field name='image' 			accessType='outputOnly' type='SFImage'/>
		<field  name='NumUsers'			accessType='outputOnly' type='SFInt32'/>
		<field  name='Gesture'			accessType='outputOnly' type='SFString'/>
		<field name='JointOrientations'	accessType='outputOnly' type='MFRotation'/>
    </IOSensor>
	
    
	///////////////////////////////////
	// Bildausgabe der Kinect-Kamera //
	///////////////////////////////////
    <Foreground>      
        <PolygonOverlay positions='0.85 0.75 0.99 0.75 0.99 0.97 0.85 0.97'>
            <Appearance>
				<MultiTexture>
					<PixelTexture2D DEF='image'/>
					<PixelTexture2D DEF='depth'/>
				</MultiTexture>
            
				<ComposedShader DEF='shader'>
					<field accessType='inputOutput' name='image' type='SFInt32' value='0'/>
					<field accessType='inputOutput' name='depth' type='SFInt32' value='1'/>
                
					<ShaderPart type='vertex'   url='"vp.glsl"'/>
					<ShaderPart type='fragment' url='"fp.glsl"'/>
				</ComposedShader>
            </Appearance>
		</PolygonOverlay>
	</Foreground>

	
	/////////////////////////////////////////////////////////////
	// Wichtige Definitionen fuer das Erzeugen der Handanzeige //
	/////////////////////////////////////////////////////////////
	<Group DEF='hand_gruppe'> </Group>
	
	
	//////////////////////
	// Hintergrundfarbe //
	//////////////////////
	<!-- UMGEBUNG TextureBackground -->
	<TextureBackground DEF="TexturHintergrund" containerField="children">
		<ImageTexture containerField="frontTexture" url="../img/hintergrund.jpg"/>
		<ImageTexture containerField="rightTexture" url="../img/hintergrund.jpg"/>
		<ImageTexture containerField="backTexture" url="../img/hintergrund.jpg"/>
		<ImageTexture containerField="leftTexture" url="../img/hintergrund.jpg"/>
	</TextureBackground>
	
	
	/////////////////////////////////////////////
	// Ladetext fuer die Kinectskeleterkennung //
	/////////////////////////////////////////////
	<Transform DEF='isKinectActive' scale='0.3 0.4 0.4' translation='6.3 1.7 0'>
		<Text string="Analysing.." />
	</Transform>	
	
	
	////////////////////////////////////////////////////////////////////
	// Plane-Box zur Darstellung des Bildes in eine groessere Version //
	////////////////////////////////////////////////////////////////////
	<Transform DEF='plane_box' translation='0 0 1.5'>
		<Shape>
			<Box size='8 6.9 0'/>
			<Appearance>
				<Material diffuseColor='0.2, 0.3, 0.2'/>
			</Appearance>
		</Shape>
	</Transform>
	
	/////////////////////////////////////////////////////////////
	// Zeigt die derzeitig geladenen Tags an                   //
	/////////////////////////////////////////////////////////////
	<Transform scale='0.3 0.4 2' translation='-6.4 -3.0 3' rotation="0 1 0 1.3">
		<Text DEF='tagnotice' string="Loading images..." />
	</Transform>
	
	<Transform DEF='notize_box' translation='-6.2 -3.1 2' rotation="0 1 0 1.3">
		<Shape>
			<Box
				size='4.2 0.1 0'
			/>
			<Appearance>
				<Material diffuseColor='0.2, 0.3, 0.2'/>
			</Appearance>
		</Shape>
	</Transform>
	
	// Button zeigt an ob eine Geste moeglich ist
	<Transform scale="0.5 0.5 0.05" translation="-7 3 0">
		<Shape>
			<Sphere/>
			<Appearance>
				<Material DEF='button_geste_material'
					diffuseColor='0.1 0.0 0.0'
					ambientIntensity='0.2'
					shininess='1'
					specularColor='1 1 1'
				/>
			</Appearance>
		</Shape>
	</Transform>
	<Transform scale='0.6 0.6 1' translation='-7.2 2.85 0.2'>
		<Text DEF='button_geste_text' string="+x" />
	</Transform>
	
	

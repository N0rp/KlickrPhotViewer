	<Group DEF='rahmen'>
		<Transform translation='-1.1 0 0'>
			<Shape>
				<Cylinder
					radius='0.03'
					height='2.23'
				/>
				<Appearance>
					<Material diffuseColor='0.2, 0.3, 0.2'/>
				</Appearance>
			</Shape>
		</Transform>
	
		<Transform translation='0 1.1 0' rotation='0 0 1 1.57'>
			<Shape>
				<Cylinder
					radius='0.03'
					height='2.23'
				/>
				<Appearance>
					<Material diffuseColor='0.2, 0.3, 0.2'/>
				</Appearance>
			</Shape>
		</Transform>
	
		<Transform translation='1.1 0 0'>
			<Shape>
				<Cylinder
					radius='0.03'
					height='2.23'
				/>
				<Appearance>
					<Material diffuseColor='0.2, 0.3, 0.2'/>

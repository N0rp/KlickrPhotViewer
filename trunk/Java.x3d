		</Shape>
		
		<TouchSensor DEF='touch_me_1'/>
		
		<TimeSensor DEF='zeit_box_1_links' cycleInterval='3' loop='false'/>
		<TimeSensor DEF='zeit_box_1_rechts' cycleInterval='3' loop='false'/>
		
		<OrientationInterpolator DEF='drehung_box_1_rechts' 
			key='0 0.25 0.5 0.75 1'
			keyValue='0 1 0 0, 0 1 0 1.57, 0 1 0 3.14159, 0 1 0 3.14159, 0 1 0 3.14159'
		/>
		<OrientationInterpolator DEF='drehung_box_1_links' 
			key='0 0.25 0.5 0.75 1'
			keyValue='0 1 0 3.14159, 0 1 0 1.57, 0 1 0 0, 0 1 0 0, 0 1 0 0'
		/>
	</Transform>
	//Rechts drehen
	<ROUTE fromNode='touch_me_1' 			fromField='touchTime' 			toNode='zeit_box_1_rechts'		toField='startTime'/>
 	<ROUTE fromNode='zeit_box_1_rechts' 	fromField='fraction_changed' 	toNode='drehung_box_1_rechts' 	toField='set_fraction'/>
 	<ROUTE fromNode='drehung_box_1_rechts' 	fromField='value_changed' 		toNode='box_1' 					toField='rotation'/>
	//Links drehen
 	<ROUTE fromNode='zeit_box_1_links' 		fromField='fraction_changed' 	toNode='drehung_box_1_links' 	toField='set_fraction'/>
 	<ROUTE fromNode='drehung_box_1_links' 	fromField='value_changed' 		toNode='box_1' 					toField='rotation'/>
	
	
